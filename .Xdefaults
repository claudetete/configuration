xterm*color10:          #AFD78A
! yellow
!xterm*color3:           #E8AE5B
xterm*color11:          #FFA75D
! blue
xterm*color4:           #71C5F4
xterm*color12:          #67CDE9
! magenta
xterm*color5:           #E2BAF1
xterm*color13:          #ECAEE9
! cyan
xterm*color6:           #21F1EA
xterm*color14:          #36FFFC
! white
xterm*color7:           #F1F1F1
xterm*color15:          #FFFFFF
